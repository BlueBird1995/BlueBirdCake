<!-- ジャンル検索部分テンプレート -->

<h2>商品一覧（全'<%=  %>'件）</h2>
<% @products.each do |product| %>
<table>
  <tr>
    <td><%= link_to "商品画像",product_path(product) %><br>
      <%= product.name %><br>
      <%= product.tax_included %>
    </td>
  </tr>
</table>
<% end %>
<!-- １ページに８件表示する -->
<%= paginate @products %>

<div class="pagination">
  <p>・</p>
  <p>・</p>
  <p>・</p>
  <p>・</p>
</div>
