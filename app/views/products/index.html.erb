<!-- ジャンル検索部分テンプレート -->

<h2>商品一覧（全'<%= @content %>'件）</h2>
<% @products.each do |product| %>
<table>
 <tr>
 	<td>
  <%= product.name %><br>
  <%= product.price*1.08 %>
</td>
</tr>
</table>
<% end %>
<!-- １ページに８件表示する -->
<%= paginate @products %>

<div class="pagination">
